{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.formatOnPaste": true,
  "editor.rulers": [100],
  "typescript.preferences.autoImportFileExcludePatterns": ["prisma"],
  "search.exclude": {
    "node_modules": true,
    ".git": true,
    "dist": true,
    "build": true
  },
  "files.watcherExclude": {
    "node_modules/**": true,
    ".git/**": true,
    "dist/**": true
  }
}
